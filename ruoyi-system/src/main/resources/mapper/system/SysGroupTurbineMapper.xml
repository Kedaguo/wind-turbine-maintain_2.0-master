<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysGroupTurbineMapper">
    
    <resultMap type="SysGroupTurbine" id="SysGroupTurbineResult">
        <result property="groupId"    column="group_id"    />
        <result property="tId"    column="t_id"    />
    </resultMap>

    <sql id="selectSysGroupTurbineVo">
        select group_id, t_id from sys_group_turbine
    </sql>

    <select id="selectSysGroupTurbineList" parameterType="SysGroupTurbine" resultMap="SysGroupTurbineResult">
        <include refid="selectSysGroupTurbineVo"/>
        <where>  
        </where>
    </select>
    
    <select id="selectSysGroupTurbineByGroupId" parameterType="Long" resultMap="SysGroupTurbineResult">
        <include refid="selectSysGroupTurbineVo"/>
        where group_id = #{groupId}
    </select>
        
    <insert id="insertSysGroupTurbine" parameterType="SysGroupTurbine">
        insert into sys_group_turbine
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="groupId != null">group_id,</if>
            <if test="tId != null">t_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="groupId != null">#{groupId},</if>
            <if test="tId != null">#{tId},</if>
         </trim>
    </insert>

    <update id="updateSysGroupTurbine" parameterType="SysGroupTurbine">
        update sys_group_turbine
        <trim prefix="SET" suffixOverrides=",">
            <if test="tId != null">t_id = #{tId},</if>
        </trim>
        where group_id = #{groupId}
    </update>

    <delete id="deleteSysGroupTurbineByGroupId" parameterType="Long">
        delete from sys_group_turbine where group_id = #{groupId}
    </delete>

    <delete id="deleteSysGroupTurbineByGroupIds" parameterType="String">
        delete from sys_group_turbine where group_id in 
        <foreach item="groupId" collection="array" open="(" separator="," close=")">
            #{groupId}
        </foreach>
    </delete>
</mapper>